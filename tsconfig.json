{
  "compilerOptions": {
    "allowJs": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "experimentalDecorators": true,
    "lib": ["es7", "es2017", "dom"],
    "baseUrl": ".",
    "paths": {
      "src/*": [ "./src/*" ],
      "components/*": [ "./src/components/*" ],
      "logic/*": [ "./src/common/logic/*" ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
